{
	"name": "zdk-Markdown-Viewer",
	"main": "index.htm",
	"version": "0.2.0-dev",
	"description": "A node-webkit application to read markdown files",
	"author": "zedesk.net <dev@zedesk.net>",
	"contributors": [
	{
		"name": "Fabrice Le Coz",
		"email": "fabrice.lecoz@zedesk.net"
	}
	],
	"keywords": [
		"node-webkit",
		"zedesk.net",
		"markdown"
	],
	"dependencies": {},
	"single-instance":true,
	"chromium-args": "--enable-threaded-compositing --enable-html-imports --remote-debugging-port=9222",
	"window": {
		"title": "zeDesk markdown",
		"icon": "img/MMD.png",
		"toolbar": false,
		"frame": true,
		"width": 800,
		"height": 900,
		"position": "mouse",
		"min_width": 800,
		"min_height": 600
	},
	"licence": "MIT",
	"devDependencies": {}
}
